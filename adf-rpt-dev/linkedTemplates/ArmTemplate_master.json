{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-rpt-dev"
		},
		"ls_dev_rpt_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_dev_rpt'"
		},
		"ls_dev_stg_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_dev_stg'"
		},
		"ls_dev_rpt_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "bc-sdg-reporting-dev.database.windows.net"
		},
		"ls_dev_rpt_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "reporting"
		},
		"ls_dev_rpt_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa_bc_sdg"
		},
		"ls_dev_stg_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "bc-sdg-reporting-dev.database.windows.net"
		},
		"ls_dev_stg_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "staging"
		},
		"ls_dev_stg_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa_bc_sdg"
		},
		"trigger1_properties_Earthquakes_parameters_starttime": {
			"type": "string",
			"defaultValue": "@utcNow('d')"
		},
		"trigger1_properties_Earthquakes_parameters_endtime": {
			"type": "string",
			"defaultValue": "@adddays(utcNow('d'), 1, 'd')"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-rpt-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dev_rpt_password": {
						"value": "[parameters('ls_dev_rpt_password')]"
					},
					"ls_dev_stg_password": {
						"value": "[parameters('ls_dev_stg_password')]"
					},
					"ls_dev_rpt_properties_typeProperties_server": {
						"value": "[parameters('ls_dev_rpt_properties_typeProperties_server')]"
					},
					"ls_dev_rpt_properties_typeProperties_database": {
						"value": "[parameters('ls_dev_rpt_properties_typeProperties_database')]"
					},
					"ls_dev_rpt_properties_typeProperties_userName": {
						"value": "[parameters('ls_dev_rpt_properties_typeProperties_userName')]"
					},
					"ls_dev_stg_properties_typeProperties_server": {
						"value": "[parameters('ls_dev_stg_properties_typeProperties_server')]"
					},
					"ls_dev_stg_properties_typeProperties_database": {
						"value": "[parameters('ls_dev_stg_properties_typeProperties_database')]"
					},
					"ls_dev_stg_properties_typeProperties_userName": {
						"value": "[parameters('ls_dev_stg_properties_typeProperties_userName')]"
					},
					"trigger1_properties_Earthquakes_parameters_starttime": {
						"value": "[parameters('trigger1_properties_Earthquakes_parameters_starttime')]"
					},
					"trigger1_properties_Earthquakes_parameters_endtime": {
						"value": "[parameters('trigger1_properties_Earthquakes_parameters_endtime')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}